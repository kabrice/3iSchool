<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}FasttCourse{% endblock %}</title>
        {% block stylesheets %}
            <meta charset="UTF-8">  
            <meta name="viewport" content="width=device-width, initial-scale=1.0">

            <link rel="stylesheet" type="text/css"  href="{{ asset('bower/bootstrap/dist/css/bootstrap.min.css') }}">
            <link rel="stylesheet" type="text/css"  href="{{ asset('bower/components-font-awesome/css/font-awesome.min.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('bower/ng-img-crop/compile/minified/ng-img-crop.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('bower/angular-rateit/dist/ng-rateit.css') }}">

            <link rel="stylesheet" type="text/css" href="{{ asset('bower/ng-dialog/css/ngDialog.min.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('bower/ng-dialog/css/ngDialog-theme-default.min.css') }}">

            <link rel="stylesheet" type="text/css"  href="https://fonts.googleapis.com/icon?family=Material+Icons">
            <!-- Important Owl stylesheet -->
            <link rel="stylesheet" type="text/css"  href="{{ asset('owl-carousel/owl.carousel.css') }}" /> 
                                                                                                          <!-- Default Theme -->
            <link rel="stylesheet" type="text/css"  href="{{ asset('owl-carousel/owl.theme.css') }}" /> 

            <link rel="stylesheet" type="text/css"  href="{{ asset('css/posterPensee.css') }}" />
            <link rel="stylesheet" type="text/css"  href="{{ asset('css/mgSuggestion.css') }}" /> 
            <link rel="stylesheet" type="text/css"  href="{{ asset('css/pdfviewer.css') }}" /> 
            <link rel="stylesheet" type="text/css"  href="{{ asset('css/sidebar.css') }}" /> 
            <link rel="stylesheet" type="text/css" href="{{ asset('css/bienvenue.css') }}" rel="stylesheet" type="text/css"/>
            <link rel="stylesheet" href="{{ asset('bower/Croppie/croppie.css') }}"  type="text/css">
        <style>
            iframe {
                height: 100% !important;
                width: 100% !important;
                overflow: hidden
            }
            .cropArea {
                background: #E4E4E4;
                overflow: hidden;
                width:100%;
                height:300px;
            }
            form .progress {
                line-height: 15px;
            }
            .progress {
                display: inline-block;
                width: 100px;
                border: 3px groove #CCC;
            }
            .progress div {
                font-size: smaller;
                background: orange;
                width: 0;
            }
        </style>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body ng-app="3ischool">
        {% block body %}

        {% endblock %}

        {% block javascripts %}

            <script type="text/javascript" src="{{ asset('bower/tinymce/tinymce.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/jquery/dist/jquery.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/jquery.cookie/jquery.cookie.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/bootstrap/dist/js/bootstrap.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular/angular.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular-sanitize/angular-sanitize.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular-resource/angular-resource.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular-filter/dist/angular-filter.js') }}" ></script>
            <script type="text/javascript" src="{{ asset('bower/angular-click-outside/clickoutside.directive.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular-animate/angular-animate.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular-ui-tinymce/src/tinymce.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/ng-file-upload/ng-file-upload.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/ng-img-crop/compile/minified/ng-img-crop.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular-recaptcha/release/angular-recaptcha.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular-bootstrap/ui-bootstrap-tpls.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/angular-rateit/dist/ng-rateit.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bower/ng-dialog/js/ngDialog.min.js') }}"></script>

            <script src="https://www.google.com/recaptcha/api.js?onload=vcRecaptchaApiLoaded&render=explicit" async defer></script>
            <script type="text/javascript" src="{{ asset('owl-carousel/owl.carousel.min.js') }}"></script>

            <script type="text/javascript" src="{{ asset('js/angular-app/app.js') }}" ></script>
            <script type="text/javascript" src="{{ asset('js/angular-app/MesDirectives.js') }}" ></script>
            <script type="text/javascript" src="{{ asset('js/angular-app/ContenuServiceHttp.js') }}" ></script>
            <script type="text/javascript" src="{{ asset('js/angular-app/ContenuServiceMock.js') }}" ></script>
            <script type="text/javascript" src="{{ asset('js/angular-app/ContenuServiceRest.js') }}" ></script>
            <script type="text/javascript" src="{{ asset('js/angular-app/MesFiltres.js') }}" ></script>

            <script type="text/javascript" src="{{ asset('js/votePensee.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/posterPensee.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/contribution.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/countLimit.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/commentaire.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/showmore.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/notification.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/sidebar.js') }}"></script>

        {% endblock %}
    </body>
</html>
